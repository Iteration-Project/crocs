{
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:react/recommended",
    "plugin:jsx-a11y/recommended",
    "plugin:react-hooks/recommended",
    "prettier"
  ],
  "rules": {
    "strict": ["error", "never"],
    "react/prop-types": 0,
    "react/react-in-jsx-scope": 0,
    "valid-typeof": 2,
    "no-unsafe-negation": 2,
    "no-unused-vars": 2,
    "no-unexpected-multiline": 2,
    "no-undef": 2
  },
  "plugins": ["react", "import", "jsx-a11y"],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
